    <SolidColorBrush
        x:Key="ProjectsPageIconTextColor"
        Opacity="0.35"
        Color="#1A1A1A" />
    <LinearGradientBrush x:Key="CreateProjectButtonBackground" StartPoint="0.6, 1" EndPoint="0.3, -1.3877788E-17">
